{
  "name": "zip-online",
  "version": "1.2.0",
  "description": "zip your files online.",
  "private": true,
  "scripts": {
    "serve": "npx parcel public/index.html --cache-dir node_modules/.cache/parcel",
    "build": "npx parcel build public/index.html --cache-dir node_modules/.cache/parcel --detailed-report --no-source-maps --public-url ./",
    "dev": "npx rimraf node_modules/.cache dist && npm run serve",
    "prebuild": "rm -rf dist",
    "checkdep": "npx npm-check-updates"
  },
  "keywords": [],
  "author": "tingyuan",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@vue/component-compiler-utils": "^3.1.1",
    "cssnano": "^4.1.10",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-html": "^6.0.0",
    "express": "^4.17.1",
    "find-cache-dir": "^3.2.0",
    "fs-extra": "^8.1.0",
    "git-rev-sync": "^2.0.0",
    "got": "^10.6.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "he": "^1.2.0",
    "jsdom": "^16.2.0",
    "mustache": "^4.0.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-local": "^1.1.0",
    "standardx": "^5.0.0",
    "validate-element-name": "^2.1.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "immer": "^8.0.1",
    "main.css": "^2.0.0",
    "neverland": "^3.1.7",
    "normalize.css": "^8.0.1"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "browser": true,
      "serviceworker": true
    },
    "globals": {
      "_": true,
      "t": true,
      "$": true
    },
    "overrides": [
      {
        "files": [
          "*.htm"
        ],
        "rules": {
          "no-labels": "off",
          "no-unused-vars": ["error", { "varsIgnorePattern": "^\\$.+" }]
        }
      }
    ]
  },
  "alias": {
    "img": "./src/img"
  },
  "config": {
    "i18n": false,
    "prerender": true,
    "inlineSize": 12000,
    "publicPath": "/zip-files/",
    "dist": "dist"
  },
  "localparcelplugin": "./public/htm-plugin.js"
}
