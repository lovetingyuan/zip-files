{
  "name": "zip-online",
  "version": "1.0.3",
  "description": "zip your files online.",
  "private": true,
  "scripts": {
    "serve": "npx parcel public/index.html --cache-dir node_modules/.cache/parcel",
    "build": "npx parcel build public/index.html --cache-dir node_modules/.cache/parcel --detailed-report --no-source-maps --public-url ./",
    "dev": "npx rimraf node_modules/.cache && npx gulp clean && npm run serve",
    "lint": "npx standard --plugin html 'src/**/*.{js,htm}' --fix",
    "prebuild": "npm run lint && npx gulp clean",
    "postbuild": "npx gulp",
    "checkdep": "npx npm-check-updates"
  },
  "keywords": [],
  "author": "tingyuan",
  "license": "MIT",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "eslint-plugin-html": "^6.0.0",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "git-rev-sync": "^1.12.0",
    "got": "^10.0.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "jsdom": "^15.2.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-lighterhtm": "./public/parcel-plugins",
    "standard": "^14.3.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lighterhtml": "^2.0.8",
    "main.css": "^2.0.0",
    "mobx": "^5.15.0",
    "normalize.css": "^8.0.1"
  },
  "standard": {
    "globals": []
  },
  "alias": {
    "img": "./src/img"
  },
  "_config": {
    "i18n": false,
    "prerender": {
      "ignoreRequests": "google-analytics"
    },
    "inlineSize": 10000,
    "publicPath": "/zip-files/"
  }
}
